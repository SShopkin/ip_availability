package socketexam;

import java.util.Scanner;

public class Client {
	
	public static void main(String[] args){
		
		
		final Scanner scanner = new Scanner(System.in);
				while (scanner.hasNextLine()) {
					final String line = scanner.nextLine();
					System.out.println(line);
				}
				scanner.close();
	}
	
	
}
